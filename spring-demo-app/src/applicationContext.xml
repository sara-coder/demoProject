<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<context:property-placeholder location="classpath:sport.properties"/>
    <!-- Define your beans here -->
    <bean id="myFortune" class ="luv2code.com.HappyFortune"></bean>
    
    <bean id="myCoach" class ="luv2code.com.TrackCoach">
    <constructor-arg ref="myFortune"></constructor-arg>
    </bean>
     <bean id="myCricketCoach" class ="luv2code.com.CricketCoach">
     <property name="fortuneService" ref="myFortune"></property>
     <property name="email" value="${foo.email}"></property>
     <property name="teamName" value="${foo.teamName}"></property>
     </bean>
</beans>





